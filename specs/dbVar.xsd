    <xs:simpleType name="ValidationResultCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for Validation result                  
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Fail"/>
            <xs:enumeration value="Pass"/>
            <xs:enumeration value="Inconclusive"/>
        </xs:restriction>
    </xs:simpleType>
