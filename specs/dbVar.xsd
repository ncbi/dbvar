<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:simpleType name="positive-decimal">
        <xs:annotation>
            <xs:documentation>The positive-decimal type specifies a positive decimal value.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minExclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AlignmentCV">
        <xs:restriction base="xs:string">
            <xs:enumeration value="First Pass"/>
            <xs:enumeration value="Second Pass"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AnalysisTypeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for Analysis Type
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="BAC assembly"/>
            <xs:enumeration value="Curated"/>
            <xs:enumeration value="de novo and local sequence assembly"/>
            <xs:enumeration value="de novo sequence assembly"/>
            <xs:enumeration value="Genotyping"/>
            <xs:enumeration value="Local sequence assembly"/>
            <xs:enumeration value="Manual observation"/>
            <xs:enumeration value="MCD analysis"/>
            <xs:enumeration value="Merging"/>
            <xs:enumeration value="Multiple"/>
            <xs:enumeration value="Not provided"/>
            <xs:enumeration value="One end anchored assembly"/>
            <xs:enumeration value="Optical mapping"/>
            <xs:enumeration value="Other"/>
            <xs:enumeration value="Paired-end mapping"/>
            <xs:enumeration value="Probe signal intensity"/>
            <xs:enumeration value="Read depth"/>
            <xs:enumeration value="Read depth and paired-end mapping"/>
            <xs:enumeration value="Sequence alignment"/>
            <xs:enumeration value="SNP genotyping analysis"/>
            <xs:enumeration value="Split read and paired-end mapping"/>
            <xs:enumeration value="Split read mapping"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BreakpointPairCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary to identify if this is From or To in a breakpoint pair               
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="From"/>
            <xs:enumeration value="To"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ClinicalSignificanceCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for clinical_significance: Keeping only to support ISCA studies. Other studies should NOT have clinical_significance.
                Studies with clinical_significance should be submitted directly to ClinVar.    
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Affects"/>
            <xs:enumeration value="Benign"/>
            <xs:enumeration value="Likely benign"/>
            <xs:enumeration value="Likely pathogenic"/>
            <xs:enumeration value="Pathogenic"/>
            <xs:enumeration value="Uncertain significance"/>
            <xs:enumeration value="not provided"/>
            <xs:enumeration value="confers sensitivity"/>
            <xs:enumeration value="drug response"/>
            <xs:enumeration value="risk factor"/>
            <xs:enumeration value="association"/>
            <xs:enumeration value="protective"/>
            <xs:enumeration value="other"/>
            <xs:enumeration value="conflicting data from submitters"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CollectionCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for subject collection              
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="1000 Genomes">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in 1000 Genomes                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Autism"> </xs:enumeration>
            <xs:enumeration value="CEPH">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in CEPH panel                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="HapMap">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in International HapMap Project               
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="HGDP">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in Human Genome Diversity Project                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NINDS">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in National Institute of Neurological Disorders and Stroke repository
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OPGP">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in Ontario Population Genomics Platform
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PDR">
                <xs:annotation>
                    <xs:documentation>
                        Individual included in Polymorphism Discovery Resource                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactRoleCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for contact type    
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Submitter"/>
            <xs:enumeration value="First author"/>
            <xs:enumeration value="Senior author"/>
            <xs:enumeration value="Curator"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExperimentTypeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for Experiment Type    
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Discovery">
                <xs:annotation>
                    <xs:documentation>
                        Experiment was used to discover structural variants               
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Validation">
                <xs:annotation>
                    <xs:documentation>
                        Experiment was used to validate structural variants                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Genotyping"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GenomicPlacementMethodCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for genomic placement methods, default is genomic placements received from submitter.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Remapped">
                <xs:annotation>
                    <xs:documentation>
                        Original placement was mapped to another assembly using NCBI assembly-to-assembly remapping software. 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Remapped ad-hoc">
                <xs:annotation>
                    <xs:documentation>
                        Original placement was mapped to another assembly by ad-hoc method at NCBI.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Submitted artifact">
                <xs:annotation>
                    <xs:documentation>
                        Original placement was on an unplaced contig which is an artifact of the assembly.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Submitted cytogenetic">
                <xs:annotation>
                    <xs:documentation>
                        Variant location was identified by cytoband, which were placed on genome by NCBI. 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Submitted genomic">
                <xs:annotation>
                    <xs:documentation>
                        Original genomic placement provided by submitter.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Submitted HGVS">
                <xs:annotation>
                    <xs:documentation>
                        Variant location was identified by HGVS nomenclature, in the hgvs_name field.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Remap failed">
                <xs:annotation>
                    <xs:documentation>
                        Original placement did not successfully remap to specified assembly.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Submitted without placement on assembly">
                <xs:annotation>
                    <xs:documentation>
                        Variant could not be placed on submitted assembly.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Submitted on non-INSDC assembly">
                <xs:annotation>
                    <xs:documentation>
                        Submitted placement is on an assembly that is not in INSDC. Placements cannot be validated or loaded.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LinkDbCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for database Link, for an archive, phenotype, or sample resource                
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="AE"/>
            <xs:enumeration value="BT"/>
            <xs:enumeration value="BioProj"/>
            <xs:enumeration value="BioSD"/>
            <xs:enumeration value="ClinGen"/>
            <xs:enumeration value="ClinVar"/>
            <xs:enumeration value="CLONE"/>
            <xs:enumeration value="CORIELL"/>
            <xs:enumeration value="COSMIC"/>
            <xs:enumeration value="dbGaP"/>
            <xs:enumeration value="dbSNP"/>
            <xs:enumeration value="dbSNP-batch"/>
            <xs:enumeration value="dbVar"/>
            <xs:enumeration value="DDBJ"/>
            <xs:enumeration value="DGV"/>
            <xs:enumeration value="DO"/>
            <xs:enumeration value="EGA"/>
            <xs:enumeration value="ENA"/>
            <xs:enumeration value="GENBANK"/>
            <xs:enumeration value="GENE"/>
            <xs:enumeration value="GeneReviews"/>
            <xs:enumeration value="GEO"/>
            <xs:enumeration value="HP"/>
            <xs:enumeration value="JAX"/>
            <xs:enumeration value="MedGen"/>
            <xs:enumeration value="MeSH"/>
            <xs:enumeration value="MONDO"/>
            <xs:enumeration value="OMIM"/>
            <xs:enumeration value="Orphanet"/>
            <xs:enumeration value="PROBE"/>
            <xs:enumeration value="PubMed"/>
            <xs:enumeration value="SNOMED"/>
            <xs:enumeration value="SRA"/>
            <xs:enumeration value="TRACE"/>
            <xs:enumeration value="UMLS"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MethodTypeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for Method Type
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="BAC aCGH"/>
            <xs:enumeration value="Curated"/>
            <xs:enumeration value="Digital array"/>
            <xs:enumeration value="FISH"/>
            <xs:enumeration value="Gene expression array"/>
            <xs:enumeration value="Karyotyping"/>
            <xs:enumeration value="MAPH"/>
            <xs:enumeration value="MassSpec"/>
            <xs:enumeration value="Merging"/>
            <xs:enumeration value="Microsatellite genotyping"/>
            <xs:enumeration value="MLPA"/>
            <xs:enumeration value="Multiple complete digestion"/>
            <xs:enumeration value="Multiple"/>
            <xs:enumeration value="Not provided"/>
            <xs:enumeration value="Oligo aCGH"/>
            <xs:enumeration value="Optical mapping"/>
            <xs:enumeration value="PCR"/>
            <xs:enumeration value="qPCR"/>
            <xs:enumeration value="ROMA"/>
            <xs:enumeration value="RT-PCR"/>
            <xs:enumeration value="Sequencing"/>
            <xs:enumeration value="SNP array"/>
            <xs:enumeration value="Southern"/>
            <xs:enumeration value="Western"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OriginCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for Variant_call Origin                  
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Not tested"/>
            <xs:enumeration value="de novo"/>
            <xs:enumeration value="Maternal"/>
            <xs:enumeration value="Paternal"/>
            <xs:enumeration value="Present in both parents"/>
            <xs:enumeration value="Tested - inconclusive"/>
            <xs:enumeration value="Somatic"/>
            <xs:enumeration value="Germline"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="PhenotypeLink">
        <xs:complexContent>
            <xs:extension base="LinkType">
                <xs:attribute name="medgen_id" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="PlacementRankCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for ranking placements
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="BestAvailable">
                <xs:annotation>
                    <xs:documentation>
                        This is the best placement available in the assembly.
                        (Regardless of whether it is submitted or remapped, single or multiple, first or second pass, genomic, cytogenetic, or HGVS)
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NotBest">
                <xs:annotation>
                    <xs:documentation>
                        There are better placements on the same assembly.    
                        Indicates that this is a remapped placement that does not have the best coverage score.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Ad-hocRemapping">
                <xs:annotation>
                    <xs:documentation>
                        This remmapped placement was generated using an unknown remapper algorithm, therefore of low confidence.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Artifact">
                <xs:annotation>
                    <xs:documentation>
                        This placement is a submitted artifact, therefore of low confidence.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NoPlacement">
                <xs:annotation>
                    <xs:documentation>
                        There is no placement submitted or remapped on the assembly.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PlacementsPerAssemblyCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary indicating if a placement is single or multiple within the same assembly.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Single">
                <xs:annotation>
                    <xs:documentation>
                        This is the only placement in this assembly.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Multiple">
                <xs:annotation>
                    <xs:documentation>
                        This is one of multiple placements in the same assembly.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RemapFailureCodeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for remap failures
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Expanded"/>
            <xs:enumeration value="LowCov"/>
            <xs:enumeration value="NoAlign"/>
            <xs:enumeration value="IDNotInAssembly"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RecipientStatusCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for placement Recipient                
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Ambiguous"/>
            <xs:enumeration value="Known"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ReferenceTypeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for reference_type               
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Assembly"/>
            <xs:enumeration value="Control tissue"/>
            <xs:enumeration value="Other"/>
            <xs:enumeration value="Sampleset"/>
            <xs:enumeration value="Sample"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SamplesetTypeCV">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Case">
                <xs:annotation>
                    <xs:documentation>
                        Sampleset was Case-set            
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Control">
                <xs:annotation>
                    <xs:documentation>
                        Sampleset was control-set               
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SeqTypeCV">
        <xs:annotation>
            <xs:documentation>Controlled vocabulary for sequence type    
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="3_inner_flank">
                <xs:annotation>
                    <xs:documentation>
                        last landmark affected by variant                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3_outer_flank">
                <xs:annotation>
                    <xs:documentation>
                        first unaffected landmark after variant                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5_outer_flank">
                <xs:annotation>
                    <xs:documentation>
                        last unaffected landmark before variant                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5_inner_flank">
                <xs:annotation>
                    <xs:documentation>
                        first landmark affected by variant                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="evidence_seq">
                <xs:annotation>
                    <xs:documentation>
                        typically a sequence trace that shows the variant breakpoint, and which was used to make the variant call                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="support_seq">
                <xs:annotation>
                    <xs:documentation>
                        typically the ID(s) of a probe(s) which were observed to have increased or decreased signal intensity, and so were used to call the variant                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="variant_seq">
                <xs:annotation>
                    <xs:documentation>
                        the actual variant sequence; can either be literal sequence under 100bp, or a GENBANK accession containing the literal sequence if greater than 100bp                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SexCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for subject Sex                  
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Female"/>
            <xs:enumeration value="Male"/>
            <xs:enumeration value="Unknown"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StrandTypeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for strand Type                 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="+">
                <xs:annotation>
                    <xs:documentation>
                        + strand             
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="-">
                <xs:annotation>
                    <xs:documentation>
                        - strand              
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="not reported">
                <xs:annotation>
                    <xs:documentation>
                        This value can be assigned by the converter if a value for strand is expected but was not submitted.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StudyTypeCV">
        <xs:annotation>
            <xs:documentation>
                Controlled vocabulary for Study Type                 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Case-Control">
                <xs:annotation>
                    <xs:documentation>
                        Comparison between healthy and diseased individuals.               
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Case-Set">
                <xs:annotation>
                    <xs:documentation>
                        Disease-associated variation.                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Collection">
                <xs:annotation>
                    <xs:documentation>
                        A collection of variation                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Control Set">
                <xs:annotation>
                    <xs:documentation>
                        Variation in healthy population.                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Somatic">
                <xs:annotation>
                    <xs:documentation>
                        Somatic Variation               
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Tumor vs. Matched-Normal">
                <xs:annotation>
                    <xs:documentation>
                        Somatic variation detected from comparison of tumor to healthy tissue from the same individual.                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SubjectAgeUnitsCV">
