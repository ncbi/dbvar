            <xs:enumeration value="deletion"/>
            <xs:enumeration value="mobile element deletion"/>
            <xs:enumeration value="alu deletion"/>
            <xs:enumeration value="line1 deletion"/>
            <xs:enumeration value="sva deletion"/>
            <xs:enumeration value="herv deletion"/>
            <xs:enumeration value="duplication"/>
            <xs:enumeration value="indel"/>
            <xs:enumeration value="insertion"/>
            <xs:enumeration value="interchromosomal translocation"/>
            <xs:enumeration value="intrachromosomal translocation"/>
            <xs:enumeration value="inversion"/>
            <xs:enumeration value="mobile element insertion"/>
            <xs:enumeration value="alu insertion"/>
            <xs:enumeration value="herv insertion"/>
            <xs:enumeration value="line1 insertion"/>
            <xs:enumeration value="sva insertion"/>
            <xs:enumeration value="novel sequence insertion"/>
