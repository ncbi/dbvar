                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Year">
                <xs:annotation>
                    <xs:documentation>
                        Subject Age in Years           
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GDay">
                <xs:annotation>
                    <xs:documentation>
                        Subject Gestational Age in Days                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GWeek">
                <xs:annotation>
                    <xs:documentation>
                        Subject Gestational Age in Weeks                 
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GMonth">
                <xs:annotation>
                    <xs:documentation>
                        Subject Gestational Age in Months                
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
